{
    "manifest_version"  :   2,
    "name"  :   "selectLookup",
    "version"   :   "1.0",
    "description"   :   "Adds a select Lookup Menu to all webpages matching mozilla.org",
    "icons" :   {
        "48"    :   "icons/border-48.png"
    },
    "content_scripts"   :   [
        {
            "matches"   :   ["*://*/*"],
            "js"    :   ["lookUp.js","lookUpRun.js"],
            "css"   :   ["style.css"]

        }
    ]
}